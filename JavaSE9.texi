@cdindex @r{<@i{Static Method isIn}>}, definition
@noindent
@r{<@i{Static Method isIn}>} @equiv{}
@example
static <T extends Comparable<T>, V extends T> boolean isIn(T x, V[] y) @{

  for (int i = 0; i < y.length; i++)
    if (x.equals(y[i]) return true;

  return false;
@}

